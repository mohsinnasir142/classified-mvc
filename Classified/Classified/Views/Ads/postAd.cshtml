@section scripts{
<script type="text/javascript">

    $(document).ready(function () {

        var json = {
            "controls": [
                  { "text": "Enter the username", "nodeType": "text", "nodeValue": "" },
                  { "text": "select brand", "nodeType": "select", "nodeValue": "LG,SAMSUNG,NOKIA" },
                  { "text": "Enter the password", "nodeType": "password", "nodeValue": "" }
            ]
        };

        var jsnObj = json.controls;
        for (var i = 0; i < jsnObj.length ; i++)
        {
            if (jsnObj[i].nodeType == 'text')
            {
                $('#controls').append('<tr> <td> <label> '+ jsnObj[i].text +' </label>    </td> <td> <input type="text" controlType="unCommon" name="RAM" />  </td>  </tr>')
            }

            else if (jsnObj[i].nodeType == 'password') {
                $('#controls').append('<tr> <td> <label> ' + jsnObj[i].text + ' </label>    </td> <td> <input type="password" controlType="unCommon" name="password" />  </td>  </tr>')
            }
            else if (jsnObj[i].nodeType == 'select') {
                var markup = "";
                for (j = 0; j < jsnObj[i].nodeValue.split(',').length; j++) {
                    markup = markup + '<option value= ' + jsnObj[i].nodeValue.split(',')[j] + ' > ' + jsnObj[i].nodeValue.split(',')[j] + ' </option>'
                }

                $('#controls').append('<tr> <td> <label> ' + jsnObj[i].text + ' </label>    </td> <td> <select controlType="unCommon" name="Brand" > ' + markup + ' </select> </td>  </tr>')
            }       }

    });

</script>
    }